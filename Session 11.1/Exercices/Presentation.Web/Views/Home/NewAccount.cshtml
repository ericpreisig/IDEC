<h1>Création d'un nouveau compte</h1>

<form asp-action="Register">
    <div>
        <label>Login</label>
        <input type="text" name="login" />
    </div>
    <div>
        <label>Mot de passe</label>
        <input type="password" name="password" />
    </div>
    <button>S'inscrire</button>
</form>